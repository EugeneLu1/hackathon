{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "clean:all": "npm-run-all analyze:clean test:clean build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run typecheck && npm run lint:css && npm run lint:eslint",
    "lint:css": "stylelint \"app/**/*.{js,ts,tsx,jsx}\"",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern app/",
    "lint:eslint:fix": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --fix app/",
    "typecheck": "tsc --noEmit",
    "prettify": "prettier --write",
    "commit": "npm-run-all extract-intl lint test:clean test build:clean build"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
